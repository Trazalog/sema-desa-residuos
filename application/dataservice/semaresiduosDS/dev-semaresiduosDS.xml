   <query id="generadoresGet" useConfig="semaresiduosDS">
      <sql>select sotr_id, razon_social, cuit, domicilio, num_registro, lat, lng, zona_id, rubr_id, tist_id, tica_id&#xd;  from log.solicitantes_transporte</sql>
